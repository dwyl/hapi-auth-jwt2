  "repository": {
    "type": "git",
    "url": "https://github.com/dwyl/hapi-auth-jwt2.git"
  },
  "keywords": [
    "Hapi.js",
    "Authentication",
    "Auth",
    "JSON Web Tokens",
    "JWT"
  ],
  "author": "@nelsonic <contact.nelsonic@gmail.com> (https://github.com/nelsonic)",
